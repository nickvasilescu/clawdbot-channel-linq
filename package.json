{
  "name": "@clawdbot/linq",
  "version": "0.1.0",
  "description": "iMessage, RCS, and SMS via Linq Partner API — no Mac required",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/types/",
    "clawdbot.plugin.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepare": "test -d dist || npm run build",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "clawdbot": {
    "extensions": [
      "./dist/index.js"
    ],
    "channel": {
      "id": "linq",
      "label": "Linq",
      "selectionLabel": "Linq (iMessage/RCS/SMS via API)",
      "blurb": "iMessage, RCS, and SMS without a Mac — via Linq Partner API.",
      "order": 50,
      "aliases": ["linq", "imessage-cloud"],
      "systemImage": "message.fill"
    },
    "install": {
      "npmSpec": "@clawdbot/linq",
      "defaultChoice": "npm"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nickvasilescu/clawdbot-channel-linq.git"
  },
  "homepage": "https://github.com/nickvasilescu/clawdbot-channel-linq#readme",
  "bugs": {
    "url": "https://github.com/nickvasilescu/clawdbot-channel-linq/issues"
  },
  "keywords": [
    "clawdbot",
    "clawdbot-plugin",
    "clawdbot-channel",
    "linq",
    "imessage",
    "rcs",
    "sms"
  ],
  "author": "clawdbot contributors",
  "dependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT"
}
